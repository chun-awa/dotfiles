{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "file-raw",
        "source": "~/.config/fastfetch/logo.txt"
    },
    "modules": [
        "title",
        "separator",
        "os",
        "host",
        "kernel",
        "uptime",
        "loadavg",
        "packages",
        "shell",
        "display",
        "de",
        "wm",
        "wmtheme",
        "theme",
        "icons",
        "font",
        "terminal",
        "terminalfont",
        {
            "type": "cpu",
            "showPeCoreCount": true
        },
        "gpu",
        "memory",
        "swap",
        {
            "type": "disk",
            "showSubvolumes": true
        },
        "battery",
        "users",
        "physicaldisk",
        "break",
        "colors"
    ]
}
